{
  "name": "@wener/utils",
  "version": "1.0.0-alpha.16",
  "description": "Utils for daily use",
  "keywords": [
    "utils",
    "core library",
    "helper"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wener/apis.git"
  },
  "license": "MIT",
  "author": "wener",
  "main": "lib/index.js",
  "source": "src/index.ts",
  "typings": "lib/index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "lib",
    "dist",
    "src",
    "README.md",
    "rollup.config.ts",
    "jest.config.js",
    "jest.setup.js",
    "tsconfig.rollup.json",
    "tsconfig.jest.json",
    "tsconfig.json"
  ],
  "scripts": {
    "prebuild": "rm -rf ./lib ./dist",
    "build": "yarn build:js && yarn build:bundle && yarn build:docs",
    "build:bundle": "echo rollup -c rollup.config.ts",
    "build:docs": "typedoc --out docs --target es6 --theme minimal --mode file src",
    "build:js": "tsc",
    "doc": "typedoc --out docs --target es6 --theme minimal --mode file src",
    "prepare": "yarn build",
    "test": "jest"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^12",
    "@rollup/plugin-json": "^4",
    "@rollup/plugin-node-resolve": "^8",
    "@rollup/plugin-typescript": "^4",
    "@types/jest": "^25",
    "@types/lodash": "^4",
    "@types/node": "^12",
    "dotenv": "^8",
    "isomorphic-unfetch": "^3",
    "jest": "^26",
    "lodash": "^4",
    "rollup": "^2",
    "ts-jest": "^26",
    "tslib": "^2",
    "typedoc": "^0",
    "typescript": "^3"
  },
  "optionalDependencies": {
    "isomorphic-unfetch": "*",
    "lodash": "*"
  },
  "publishConfig": {
    "access": "public"
  }
}
