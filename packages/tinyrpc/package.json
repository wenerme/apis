{
  "name": "@wener/tinyrpc",
  "version": "1.0.0-alpha.14",
  "description": "Tiny RPC for ES6 Proxy",
  "keywords": [
    "rpc"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wener/apis.git"
  },
  "license": "MIT",
  "author": "wener",
  "main": "lib/tinyrpc.js",
  "umd:main": "dist/tinyrpc.umd.js",
  "module": "dist/tinyrpc.es5.js",
  "source": "src/tinyrpc.ts",
  "browser": "dist/tinyrpc.umd.js",
  "typings": "lib/tinyrpc.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "lib",
    "dist",
    "src",
    "README.md",
    "rollup.config.ts",
    "jest.config.js",
    "jest.setup.js",
    "tsconfig.rollup.json",
    "tsconfig.jest.json",
    "tsconfig.json"
  ],
  "scripts": {
    "prebuild": "rm -rf ./lib ./dist",
    "build:js": "tsc",
    "build:docs": "typedoc --out docs --target es6 --theme minimal --mode file src",
    "build:bundle": "rollup -c rollup.config.ts",
    "build": "yarn build:js && yarn build:bundle && yarn build:docs",
    "doc": "typedoc --out docs --target es6 --theme minimal --mode file src",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-typescript": "^4.0.0",
    "@types/jest": "^25.1.4",
    "@types/lodash.camelcase": "^4.3.6",
    "@types/rollup-plugin-json": "^3.0.2",
    "jest": "^25.1.0",
    "lodash.camelcase": "^4.3.0",
    "rollup": "^2.3.2",
    "ts-jest": "^25.2.1",
    "tslib": "^1.11.1",
    "typedoc": "^0.17.3",
    "typescript": "^3.8.3"
  },
  "optionalDependencies": {
    "isomorphic-unfetch": "^3.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
