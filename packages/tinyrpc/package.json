{
  "name": "@wener/tinyrpc",
  "version": "1.0.0-alpha.14",
  "description": "Tiny RPC for ES6 Proxy",
  "keywords": [
    "rpc"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wener/apis.git"
  },
  "license": "MIT",
  "author": "wener",
  "main": "lib/tinyrpc.js",
  "umd:main": "dist/tinyrpc.umd.js",
  "module": "dist/tinyrpc.es5.js",
  "source": "src/tinyrpc.ts",
  "browser": "dist/tinyrpc.umd.js",
  "typings": "lib/tinyrpc.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "lib",
    "dist",
    "src",
    "README.md",
    "rollup.config.ts",
    "jest.config.js",
    "jest.setup.js",
    "tsconfig.rollup.json",
    "tsconfig.jest.json",
    "tsconfig.json"
  ],
  "scripts": {
    "build": "yarn build:js && yarn build:bundle",
    "build:bundle": "rollup -c rollup.config.ts",
    "build:docs": "typedoc --out docs --target es6 --theme minimal --mode file src",
    "build:js": "tsc",
    "clean": "rm -rf ./lib ./dist",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest"
  },
  "dependencies": {
    "tslib": "*"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "latest",
    "@rollup/plugin-json": "latest",
    "@rollup/plugin-node-resolve": "latest",
    "@rollup/plugin-typescript": "latest",
    "@types/jest": "latest",
    "@types/rollup-plugin-json": "latest",
    "dotenv": "^8",
    "jest": "^26",
    "rollup": "^2.3.2",
    "ts-jest": "^25.2.1",
    "tslib": "*",
    "typedoc": "^0",
    "typescript": "^3"
  },
  "optionalDependencies": {
    "isomorphic-unfetch": "*"
  },
  "publishConfig": {
    "access": "public"
  }
}
