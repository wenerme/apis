    "gen": "TS_NODE_PROJECT=tsconfig.ts-node.json node -r ts-node/register -r tsconfig-paths/register server/scripts/prebuild.ts",
    "set-platform": "TS_NODE_PROJECT=tsconfig.ts-node.json node -r ts-node/register -r tsconfig-paths/register server/scripts/set-platform.ts",
