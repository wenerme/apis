    "build-storybook": "build-storybook",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc -p .",
    "dev": "next dev -p ${PORT:-3000}",
    "fix": "gts fix",
    "gen": "TS_NODE_PROJECT=tsconfig.ts-node.json node -r ts-node/register -r tsconfig-paths/register src/servers/scripts/prebuild.ts",
    "heroku-postbuild": "yarn run set-platform HEROKU && yarn run build",
    "icons": "yarn run icons-svgo && yarn run icons-gen",
    "icons-gen": "ts-node --project ./tsconfig.ts-node.json ./scripts/icons-generate.ts",
    "icons-svgo": "svgo -f public/icons/svg/ --config svgo.config.json",
    "now-build": "yarn --cwd ../.. install && yarn --cwd ../.. build:public && yarn run set-platform NOW && yarn run build",
    "prepare": "yarn run compile",
    "run:prettier": "yarn prettier --write __tests__ src scripts ./*.json ./*.js",
    "script:kong-lego": "ncc build ./scripts/kong-lego.ts -o dist/kong-lego && mv dist/kong-lego/index.js dist/kong-lego.js && git add dist/kong-lego.js",
    "set-platform": "TS_NODE_PROJECT=tsconfig.ts-node.json node -r ts-node/register -r tsconfig-paths/register src/servers/scripts/set-platform.ts",
    "setup-db": "node -r ts-node/register -r tsconfig-paths/register scripts/setup-db.ts",
    "sort": "npx sort-package-json",
    "start": "next start -p ${PORT:-3000}",
    "storybook": "start-storybook -p 6006",
    "pretest": "yarn run compile",
    "test": "jest",
    "posttest": "yarn run check"
