{
  "name": "@wener/ui",
  "version": "1.0.0-alpha.7",
  "license": "MIT",
  "main": "lib/index.js",
  "umd:main": "dist/ui.umd.js",
  "module": "dist/ui.es5.js",
  "source": "src/index.ts",
  "browser": "dist/ui.umd.js",
  "typings": "lib/index.d.ts",
  "files": [
    "tsconfig.ts-node.json",
    ".storybook",
    "stories",
    "lib",
    "dist",
    "src",
    "README.md",
    "rollup.config.ts",
    "jest.config.js",
    "jest.setup.js",
    "tsconfig.rollup.json",
    "tsconfig.jest.json",
    "tsconfig.json"
  ],
  "scripts": {
    "prebuild": "rm -rf ./lib ./dist",
    "build": "tsc && rollup -c rollup.config.ts && typedoc --out docs --target es6 --theme minimal --mode file src",
    "dev": "yarn run storybook",
    "docs": "typedoc --out docs --target es6 --theme minimal --mode file src",
    "format": "yarn prettier --write src stories .storybook ./*.json ./*.js",
    "lint": "eslint --fix --ext .ts,.tsx src",
    "storybook": "start-storybook -p ${PORT:-6006}",
    "storybook:build": "build-storybook -c .storybook -o storybook",
    "test": "jest"
  },
  "dependencies": {
    "@wener/utils": "^1.0.0-alpha.7",
    "polished": "^3.5.1",
    "react-fast-compare": "^3.0.1",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-typescript": "^4.0.0",
    "@storybook/addon-actions": "^5.3.17",
    "@storybook/addon-centered": "^5.3.17",
    "@storybook/addon-docs": "^5.3.17",
    "@storybook/addon-knobs": "^5.3.17",
    "@storybook/addon-links": "^5.3.17",
    "@storybook/addon-storyshots": "^5.3.17",
    "@storybook/addon-storysource": "^5.3.17",
    "@storybook/addon-viewport": "^5.3.17",
    "@storybook/addons": "^5.3.17",
    "@storybook/react": "^5.3.17",
    "@types/jest": "^25.1.4",
    "@types/node": "^12.12.31",
    "@types/react-test-renderer": "^16.9.2",
    "@types/styled-components": "^5.0.1",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "babel-plugin-styled-components": "^1.10.7",
    "dotenv": "^8.2.0",
    "jest": "^25.2.3",
    "react-docgen-typescript-loader": "^3.7.1",
    "react-test-renderer": "^16.13.1",
    "rollup": "^2.3.2",
    "ts-jest": "^25.2.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslib": "^1.11.1",
    "typedoc": "^0.17.3",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "@ant-design/icons": "^4.0.3",
    "antd": "^4.0.4",
    "lodash": "^4.17.15",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.3.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
