{
  "name": "@wener/ui",
  "version": "1.0.0-alpha.14",
  "license": "MIT",
  "main": "lib/index.js",
  "source": "src/index.ts",
  "typings": "lib/index.d.ts",
  "files": [
    "antds.js",
    "antds.d.ts",
    "icons.js",
    "icons.d.ts",
    "index.js",
    "index.d.ts",
    "scripts",
    "tsconfig.ts-node.json",
    ".storybook",
    "stories",
    "lib",
    "dist",
    "src",
    "README.md",
    "rollup.config.ts",
    "jest.config.js",
    "jest.setup.js",
    "tsconfig.rollup.json",
    "tsconfig.jest.json",
    "tsconfig.json"
  ],
  "scripts": {
    "antd-icons:gen": "ts-node --project ./tsconfig.ts-node.json ./scripts/antd-icons.ts",
    "prebuild": "rm -rf ./lib ./dist",
    "build": "npm-run-all -p build:types build:js build:docs && yarn build:bundle",
    "build:bundle": "echo rollup -c rollup.config.ts",
    "build:docs": "typedoc --out docs --target es6 --theme minimal --mode file src",
    "build:js": "babel src -d lib -x .ts,.tsx -s true",
    "build:types": "tsc --emitDeclarationOnly",
    "dev": "yarn run storybook --ci",
    "docs": "typedoc --out docs --target es6 --theme minimal --mode file src",
    "format": "yarn prettier --write src stories .storybook ./*.json ./*.js",
    "icons": "yarn run icons:svgo && yarn run icons:svgr && yarn run icons:gen && git add src/icons",
    "icons:gen": "ts-node --project ./tsconfig.ts-node.json ./scripts/icons.ts",
    "icons:svgr": "svgr -d ./src/icons/svgr/ ./src/icons/svgs/ --typescript --icon",
    "icons:svgo": "svgo -f src/icons/svgs --config svgo.config.json",
    "lint": "eslint --fix --ext .ts,.tsx src",
    "storybook": "start-storybook -p ${PORT:-6006}",
    "storybook:build": "build-storybook -c .storybook -o storybook --quiet",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check --watch"
  },
  "dependencies": {
    "@wener/utils": "^1.0.0-alpha.14",
