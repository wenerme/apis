{
  "name": "rjsf-antd-theme",
  "version": "1.0.0-alpha.15",
  "description": "Ant Design Theme for React Json Schema Form / RJSF",
  "repository": "https://github.com/wenerme/apis",
  "license": "MIT",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "tsconfig.json",
    "src",
    "index.*"
  ],
  "scripts": {
    "build": "tsc",
    "build:docs": "typedoc --out docs --target es6 --theme minimal --mode file src",
    "build:storybook": "build-storybook -c .storybook -o storybook --quiet",
    "dev": "yarn storybook",
    "storybook": "start-storybook -p ${PORT:-6006}",
    "test": "jest"
  },
  "devDependencies": {
    "@ant-design/icons": "^4",
    "@rjsf/core": "^2",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-centered": "next",
    "@storybook/addon-docs": "^6.1.11",
    "@storybook/addon-knobs": "^6.1.11",
    "@storybook/addon-storysource": "^6.1.11",
    "@storybook/addons": "^6.1.11",
    "@storybook/react": "^6.1.11",
    "@storybook/source-loader": "^6.1.11",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.16",
    "@types/react-test-renderer": "^17.0.0",
    "antd": "^4",
    "babel-loader": "^8.2.2",
    "babel-preset-react-app": "^10.0.0",
    "classnames": "^2",
    "core-js": "^3.8.1",
    "dotenv": "^8",
    "immer": "^8.0.0",
    "jest": "^26",
    "json-schema": "^0.2.5",
    "lodash": "^4",
    "moment": "^2",
    "react": "^16",
    "react-docgen-typescript-loader": "^3",
    "react-dom": "^16",
    "react-test-renderer": "^16",
    "styled-components": "^5",
    "ts-jest": "^26",
    "tsconfig-paths-webpack-plugin": "^3",
    "typedoc": "^0.17",
    "typescript": "^3"
  },
  "peerDependencies": {
    "@ant-design/icons": "*",
    "@rjsf/core": "^2",
    "antd": "^4",
    "classnames": "*",
    "lodash": "*",
    "moment": "*",
    "react": "*",
    "react-dom": "*"
  },
  "publishConfig": {
    "access": "public"
  }
}
