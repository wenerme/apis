{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/packages/server/$1"
    },
    {
      "src": "^/service-worker.js$",
      "dest": "/packages/server/_next/static/service-worker.js",
      "headers": {
        "cache-control": "public, max-age=43200, immutable",
        "Service-Worker-Allowed": "/"
      }
    }
  ],
  "builds": [
    {
      "src": "packages/server/next.config.js",
      "use": "@now/next"
    }
  ]
}
