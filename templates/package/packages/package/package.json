{
  "name": "@package/package",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "docs": "typedoc --out docs --target es6 --theme minimal --mode file src",
    "format": "yarn prettier --write src stories .storybook ./*.json ./*.js",
    "lint": "eslint --fix --ext .ts,.tsx src",
    "next:build": "next build",
    "next:dev": "next dev -p ${PORT:-3000}",
    "next:start": "next start -p ${PORT:-3000}",
    "storybook": "start-storybook -p ${PORT:-6006}",
    "storybook:build": "build-storybook -c .storybook -o storybook",
    "build": "tsc",
    "dev": "yarn storybook",
    "test": "jest"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "tsconfig.json",
    "src"
  ],
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.17",
    "@storybook/addon-centered": "^5.3.17",
    "@storybook/addon-docs": "^5.3.17",
    "@storybook/addon-knobs": "^5.3.17",
    "@storybook/addon-links": "^5.3.17",
    "@storybook/addon-storyshots": "^5.3.17",
    "@storybook/addon-storysource": "^5.3.17",
    "@storybook/addon-viewport": "^5.3.17",
    "@storybook/addons": "^5.3.17",
    "@storybook/react": "^5.3.17",
    "@types/jest": "^25.1.4",
    "@types/node": "^12.12.31",
    "@types/react-test-renderer": "^16.9.2",
    "dotenv": "^8.2.0",
    "jest": "^25.2.3",
    "react-docgen-typescript-loader": "^3.7.1",
    "react-test-renderer": "^16.13.1",
    "ts-jest": "^25.2.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typedoc": "^0.17.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
